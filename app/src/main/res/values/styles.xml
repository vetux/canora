<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Application Themes -->
    <style name="Default" parent="MarineBlue"/>

    <style name="Base" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Main Styling Attributes -->
        <item name="colorPrimary">?colorDebugUnset</item>
        <item name="colorPrimaryAlt">?colorDebugUnset</item>
        <item name="colorSecondary">?colorDebugUnset</item>
        <item name="colorSecondary_20">?colorDebugUnset</item>
        <item name="colorSecondary_60">?colorDebugUnset</item>
        <item name="colorSecondaryAlt">?colorDebugUnset</item>

        <item name="onColorPrimary">?colorDebugUnset</item>
        <item name="onColorPrimary_20">?colorDebugUnset</item>
        <item name="onColorPrimary_60">?colorDebugUnset</item>
        <item name="onColorPrimaryAlt">?colorDebugUnset</item>
        <item name="onColorSecondary">?colorDebugUnset</item>
        <item name="onColorSecondaryAlt">?colorDebugUnset</item>

        <item name="texToolbar">?colorDebugUnset</item>
        <item name="texBackground">?colorDebugUnset</item>
        <item name="texBackgroundContent">?colorDebugUnset</item>
        <item name="texControl">?colorDebugUnset</item>
        <item name="texControlFull">?colorDebugUnset</item>
        <item name="texNotification">?colorDebugUnset</item>
        <item name="texWindow">?colorDebugUnset</item>
        <item name="texSurface">?colorDebugUnset</item>
        <item name="texNavHeader">?colorDebugUnset</item>

        <item name="onTexToolbar">?colorDebugUnset</item>
        <item name="onTexBackground">?colorDebugUnset</item>
        <item name="onTexBackgroundContent">?colorDebugUnset</item>
        <item name="onTexBackgroundContent_20">?colorDebugUnset</item>
        <item name="onTexBackgroundContent_60">?colorDebugUnset</item>
        <item name="onTexControl">?colorDebugUnset</item>
        <item name="onTexControlFull">?colorDebugUnset</item>
        <item name="onTexNotification">?colorDebugUnset</item>
        <item name="onTexSurface">?colorDebugUnset</item>
        <item name="onTexNavHeader">?colorDebugUnset</item>

        <!-- These tinting values affect the corresponding textures when a dynamic bitmap is applied. Such as album artwork etc. -->
        <item name="tintTexControl">?colorDebugUnset</item>
        <item name="tintTexControlFull">?colorDebugUnset</item>

        <item name="scaleTexToolbar">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexBackground">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexBackgroundContent">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexControl">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexControlFull">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexNotification">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexWindow">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexSurface">@integer/ImageView_scaleType_fitXY</item>
        <item name="scaleTexNavHeader">@integer/ImageView_scaleType_fitXY</item>

        <!-- Debug -->
        <item name="colorDebugUnset">#ff00ff</item>

        <item name="motionSceneMain">@xml/motionscene_main</item>

        <!-- Android Attributes -->
        <!-- ListView Overscroll -->
        <item name="android:colorEdgeEffect">?colorDebugUnset</item>
        <!-- Scrollbars -->
        <item name="android:textColorSecondary">?colorDebugUnset</item>
        <!-- Fast Scroll Thumb -->
        <item name="colorAccent">?colorSecondary_60</item>

        <!-- Override this to true for dark icons on status bar, only works on API 23+ -->
        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>
    </style>

